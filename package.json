{
    "name": "mashi-coro",
    "version": "0.0.1",
    "description": "",
    "author": "Stuchlík Lukáš",
    "license": "MIT",
    "engines": {
        "node": "13.x",
        "yarn": "1.21.x"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "pretest": "npm run test:prepare",
        "pretest:debug": "npm run test:prepare",
        "test": "jest --runInBand",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm test -- --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "npm test -- --config ./test/jest-e2e.config.js",
        "test:prepare": "cross-env NODE_ENV=test npm run database:prepare",
        "typeorm": "node tsnode-typeorm",
        "database:seed": "ts-node -r tsconfig-paths/register src/database/seeds",
        "database:prepare": "npm run typeorm schema:drop && npm run typeorm migration:run && npm run database:seed"
    },
    "dependencies": {
        "@nestjs/common": "^6.10.12",
        "@nestjs/core": "^6.10.12",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.1",
        "@nestjs/platform-express": "^6.10.12",
        "@nestjs/platform-socket.io": "^6.10.14",
        "@nestjs/typeorm": "^6.2.0",
        "@nestjs/websockets": "^6.10.14",
        "apollo-server-express": "^2.9.14",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "config": "^3.2.4",
        "lodash": "^4.17.15",
        "mysql2": "^2.0.2",
        "passport": "^0.4.1",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "pg": "^7.18.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.3",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "@nestjs/cli": "6.12.6",
        "@nestjs/schematics": "^6.7.6",
        "@nestjs/testing": "^6.10.12",
        "@types/bcryptjs": "^2.4.2",
        "@types/config": "^0.0.36",
        "@types/express": "^4.17.1",
        "@types/jest": "^24.0.18",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.7.5",
        "@types/socket.io": "^2.1.4",
        "@types/supertest": "^2.0.8",
        "cross-env": "^6.0.3",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.1.1",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "tslint-config-airbnb": "^5.11.2",
        "typescript": "^3.6.3"
    }
}
